  "bugs": {
    "url": "https://github.com/pozengineer/resonateContact/issues"
  },
  "homepage": "https://github.com/pozengineer/resonateContact#readme",
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "if-env": "^1.0.4",
    "mongoose": "^6.2.6",
    "morgan": "^1.10.0"
  }
}
